    "panelTitle.inactiveForeground": "#8c9aa6",
    "panelTitle.activeBorder": "#7aa6c9",

    "badge.background": "#2f4b63",
    "badge.foreground": "#eaf4ff",

    "titleBar.activeBackground": "#0e1419",
    "titleBar.activeForeground": "#aebbcc",
    "titleBar.inactiveBackground": "#0e1419",
    "titleBar.inactiveForeground": "#8c9aa6",

    "notificationCenterHeader.background": "#111921",
    "notifications.background": "#111921",
    "notifications.foreground": "#cfe0ec",
    "notificationLink.foreground": "#7aa6c9",

    "input.background": "#121a21",
    "input.border": "#2a3641",
    "input.foreground": "#dfeaf3",
    "input.placeholderForeground": "#8c9aa6",
    "inputOption.activeBorder": "#7aa6c9",
    "inputValidation.errorBackground": "#2a1212",
    "inputValidation.errorBorder": "#e06b74",
    "inputValidation.infoBackground": "#0f2020",
    "inputValidation.infoBorder": "#5cc8c1",
    "inputValidation.warningBackground": "#2a2111",
    "inputValidation.warningBorder": "#e5b86a",

    "dropdown.background": "#121a21",
    "dropdown.border": "#2a3641",
    "dropdown.foreground": "#dfeaf3",

    "peekView.border": "#2f4b63",
    "peekViewEditor.background": "#0f161c",
    "peekViewEditor.matchHighlightBackground": "#b7473f55",
    "peekViewResult.background": "#121a21",
    "peekViewTitle.background": "#111921",

    "terminal.background": "#0e1419",
    "terminal.foreground": "#cfe0ec",
    "terminalCursor.foreground": "#a9c6dd",
    "terminal.ansiBlack": "#0e1419",
    "terminal.ansiRed": "#b7473f",
    "terminal.ansiGreen": "#7ea37a",
    "terminal.ansiYellow": "#e5b86a",
    "terminal.ansiBlue": "#7aa6c9",
    "terminal.ansiMagenta": "#ddb7c8",
    "terminal.ansiCyan": "#5cc8c1",
    "terminal.ansiWhite": "#cfe0ec",
    "terminal.ansiBrightBlack": "#2a3641",
    "terminal.ansiBrightRed": "#e06b74",
    "terminal.ansiBrightGreen": "#98c493",
    "terminal.ansiBrightYellow": "#f0cd86",
    "terminal.ansiBrightBlue": "#a9c6dd",
    "terminal.ansiBrightMagenta": "#e5c8d8",
    "terminal.ansiBrightCyan": "#7ee0d8",
    "terminal.ansiBrightWhite": "#ffffff",

    "editorWidget.background": "#111921",
    "editorWidget.foreground": "#d9e6f2",
    "editorWidget.border": "#2a3641",
    "editorSuggestWidget.highlightForeground": "#a9c6dd",
    "editorSuggestWidget.selectedBackground": "#18222b",

    "gitDecoration.addedResourceForeground": "#7ea37a",
    "gitDecoration.modifiedResourceForeground": "#7aa6c9",
    "gitDecoration.deletedResourceForeground": "#b7473f",
    "gitDecoration.untrackedResourceForeground": "#98c493",
    "gitDecoration.conflictingResourceForeground": "#ddb7c8",
    "gitDecoration.ignoredResourceForeground": "#5a6a78",

    "pickerGroup.border": "#2a3641",
    "pickerGroup.foreground": "#a9c6dd",

    "breadcrumb.background": "#0e1419",
    "breadcrumb.foreground": "#8c9aa6",
    "breadcrumb.focusForeground": "#cfe0ec",
    "breadcrumb.activeSelectionForeground": "#d9e6f2",

    "debugToolBar.background": "#111921",
    "debugIcon.breakpointForeground": "#e06b74",

    "progressBar.background": "#7aa6c9",

    "menubar.selectionBackground": "#111f27",
    "menu.background": "#111921",
    "menu.foreground": "#cfe0ec",
    "menu.selectionBackground": "#18222b",
    "menu.separatorBackground": "#2a3641",

    "notebook.cellBorderColor": "#2a3641",
    "notebook.focusedCellBorder": "#2f4b63",
    "notebook.inactiveFocusedCellBorder": "#22303a",
    "notebook.selectedCellBackground": "#161e25"
  },

  "tokenColors": [
    {
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": { "foreground": "#5a6a78", "fontStyle": "italic" }
    },
    {
      "scope": ["keyword", "storage.type", "storage.modifier"],
      "settings": { "foreground": "#b7473f", "fontStyle": "bold" }
    },
    {
      "scope": ["entity.name.type", "support.type", "support.class"],
      "settings": { "foreground": "#a9c6dd" }
    },
    {
      "scope": ["variable", "meta.definition.variable"],
      "settings": { "foreground": "#cfe0ec" }
    },
    {
      "scope": ["variable.parameter", "meta.parameter"],
      "settings": { "foreground": "#e5b86a" }
    },
    {
      "scope": [
        "entity.name.function",
        "support.function",
        "meta.function-call"
      ],
      "settings": { "foreground": "#7aa6c9" }
    },
    {
      "scope": ["string", "constant.other.symbol"],
      "settings": { "foreground": "#98c493" }
    },
    {
      "scope": ["constant.numeric", "constant.language.boolean"],
      "settings": { "foreground": "#ddb7c8" }
    },
    {
      "scope": ["punctuation", "meta.brace", "meta.delimiter"],
      "settings": { "foreground": "#b7c4cf" }
    },
    {
      "scope": ["entity.other.attribute-name", "support.constant"],
      "settings": { "foreground": "#e5b86a" }
    },
    {
      "scope": ["invalid", "invalid.illegal"],
      "settings": { "foreground": "#ffffff" }
    },
    {
      "scope": ["markup.heading"],
      "settings": { "foreground": "#e5b86a", "fontStyle": "bold" }
    },
    {
      "scope": ["markup.bold"],
      "settings": { "fontStyle": "bold" }
    },
    {
      "scope": ["markup.italic"],
      "settings": { "fontStyle": "italic" }
    },
    {
      "scope": ["markup.inline.raw", "markup.fenced_code"],
      "settings": { "foreground": "#a9c6dd" }
    },
    {
      "scope": ["diff.inserted"],
      "settings": { "foreground": "#98c493" }
    },
    {
      "scope": ["diff.deleted"],
      "settings": { "foreground": "#e06b74" }
    },
    {
      "scope": ["diff.changed"],
      "settings": { "foreground": "#7aa6c9" }
    }
  ]
}
