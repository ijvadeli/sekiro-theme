    "editor.findMatchHighlightBackground": "#b7473f33",
    "editor.findRangeHighlightBackground": "#2a364133",
    "editor.hoverHighlightBackground": "#2a364155",
    "editor.lineHighlightBackground": "#18222b",
    "editor.lineHighlightBorder": "#18222b",
    "editorWhitespace.foreground": "#2a364188",
    "editorRuler.foreground": "#22303a",
    "editorBracketMatch.background": "#2f4b6344",
