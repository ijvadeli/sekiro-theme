    "editorGroup.dropBackground": "#7aa6c933",

    "tab.activeBackground": "#111921",
    "tab.inactiveBackground": "#0e1419",
    "tab.activeForeground": "#d9e6f2",
    "tab.inactiveForeground": "#8c9aa6",
    "tab.hoverBackground": "#111f27",
    "tab.hoverForeground": "#cfe0ec",
    "tab.border": "#0e1419",
    "tab.activeBorderTop": "#7aa6c9",

    "activityBar.background": "#0e1419",
